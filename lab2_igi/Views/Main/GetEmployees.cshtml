@model List<lab1_ef.Employee>

<div><img src="~/images/logo.jpg" /></div>
@using (Html.BeginForm())
{
    <h2>EMPLOYEES</h2>
    <div>
        <ul>
            <li><p>ID, Name</p></li>
            @foreach (var type in Model)
            {
                <li>
                    <p>@type.EmployeeId, @type.Name</p>
                </li>
            }
        </ul>
    </div>
}